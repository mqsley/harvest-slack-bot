{
  "name": "harvest-slack-bot",
  "version": "0.0.1",
  "description": "custom harvest timesheet slack integration with reminders",
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:ui": "vitest run --ui",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "start:slack": "node dist/slack.js",
    "start:oauth": "node dist/oauth.js",
    "start:remind": "node dist/remind.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.3",
    "@prisma/client": "^5.21.1",
    "@slack/bolt": "^4.0.1",
    "@slack/web-api": "^7.7.0",
    "hono": "^4.6.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "dotenv": "^16.4.5",
    "prisma": "^5.21.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
